<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="PsdDao">

	<select id="select_sql" parameterType="PsdVO" resultType="PsdVO">
		select * from psdTbl order by seq
	</select>
	
	<select id="select1_sql" parameterType="PsdVO" resultType="PsdVO">
		select * from psdTbl where seq = #{seq}
	</select>
	
	<insert id="insert_sql" parameterType="PsdVO">
		insert into psdTbl (seq, title, uploadFileStr)
		values (idx.nextval,#{title}, #{uploadFileStr})
	</insert>
	
	<delete id="delete_sql" parameterType="PsdVO">
		delete from psdTbl where seq = #{seq}
	</delete>
	
</mapper>
