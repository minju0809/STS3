<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CampsiteDao">

	<select id="select_sql" parameterType="campsite" resultType="campsite">
		select * from campsiteTbl
	</select>
	
	<select id="selectOne_sql" parameterType="campsite" resultType="campsite">
		select * from campsiteTbl where idx like '%'|| #{idx} ||'%'
	</select>

</mapper>
