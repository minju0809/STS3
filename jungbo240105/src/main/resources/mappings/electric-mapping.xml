<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ElectricDao">

	<select id="select_sql" parameterType="electric" resultType="electric">
		select * from gangwonTbl
	</select>
	
	<select id="selectOne_sql" parameterType="electric" resultType="electric">
		select * from gangwonTbl where idx=#{idx}
	</select>

	<select id="selectMemo_sql" parameterType="electric" resultType="electric">
		select * from gangwonMemo where gangwonIdx=#{idx}
	</select>
	
	<insert id="insert_sql" parameterType="electric">
		insert into gangwonMemo (idx, gangwonIdx, memo, name)
		values (memoSeq.nextval, #{gangwonIdx}, #{memo}, #{name})
	</insert>
</mapper>
